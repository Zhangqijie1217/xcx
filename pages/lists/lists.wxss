.wrap {
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 20rpx;
}
navigator {
  overflow: hidden;
}
.list {
  margin-bottom: 20rpx;
  height: 200rpx;
  position: relative;
}
.imgs {
  float: left;
}
.imgs image {
  display: block;
  width: 200rpx;
  height: 200rpx;
}
.infos {
  float: left;
  width: 240rpx;
  height: 200rpx;
  padding: 20rpx 0 20rpx;
}
.date {
  font-size: 16px;
  color: #aaa;
  position: absolute;
   bottom: 0;
}